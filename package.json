{
  "name": "polymer-skeleton",
  "version": "0.0.0",
  "main": "src/index.js",
  "license": "MIT",
  "author": "LasaleFamine <info@godev.space>",
  "scripts": {
    "build": "yarn copystatic:build && webpack",
    "dev": "yarn copystatic && webpack-dev-server --hot --inline",
    "pretest": "yarn linkbower && yarn build",
    "test": "xo && wct",
    "posttest": "yarn rmbower",
    "lint": "xo --fix",
    "copystatic": "node scripts/copy-statics.js",
    "copystatic:build": "node scripts/copy-statics.js build",
    "linkbower": "node scripts/link-bower.js",
    "rmbower": "node scripts/link-bower.js remove"
  },
  "xo": {
    "space": true,
    "envs": [
      "browser"
    ],
    "rules": {
      "eol-last": 0,
      "import/no-unassigned-import": 0
    },
    "ignores": [
      "test/test-fixture.js"
    ]
  },
  "devDependencies": {
    "@polymer/test-fixture": "PolymerX/test-fixture#3.0-preview",
    "chokidar": "1.7.0",
    "cssnano": "3.10.0",
    "fs-extra": "4.0.1",
    "postcss": "6.0.11",
    "postcss-cssnext": "3.0.2",
    "postcss-loader": "2.0.6",
    "postcss-reporter": "5.0.0",
    "stylelint-config-standard": "17.0.0",
    "text-loader": "0.0.1",
    "touch": "3.1.0",
    "web-component-tester": "6.1.5",
    "webpack": "3.5.6",
    "webpack-dev-server": "2.7.1",
    "workbox-webpack-plugin": "2.0.0",
    "xo": "0.18.2"
  },
  "dependencies": {
    "@polymer/polymer": "LasaleFamine/polymer#3.0-preview"
  }
}
